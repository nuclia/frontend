{
  "version": 2,
  "projects": {
    "app": "apps/app",
    "auth": "libs/auth",
    "common": "libs/common",
    "components": "libs/components",
    "core": "libs/core",
    "desktop": "apps/desktop",
    "desktop-electron": {
      "$schema": "../../../node_modules/nx/schemas/project-schema.json",
      "root": "apps/desktop/electron",
      "sourceRoot": "apps/desktop/electron/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/desktop-electron",
            "main": "apps/desktop/electron/src/main.ts",
            "tsConfig": "apps/desktop/electron/tsconfig.app.json",
            "assets": ["apps/desktop/electron/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/desktop/electron/src/environments/environment.ts",
                  "with": "apps/desktop/electron/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "desktop-electron:build"
          }
        },
        "package": {
          "executor": "nx-electron:package",
          "options": {
            "name": "desktop-electron",
            "frontendProject": "desktop",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "executor": "nx-electron:make",
          "options": {
            "name": "desktop-electron",
            "frontendProject": "desktop",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/desktop/electron/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/desktop/electron"],
          "options": {
            "jestConfig": "apps/desktop/electron/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "manager": "apps/manager",
    "pastanaga": "libs/pastanaga",
    "sdk-core": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "libs/sdk-core",
      "sourceRoot": "libs/sdk-core/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["yarn sdk-build"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/sdk-core/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/sdk-core"],
          "options": {
            "jestConfig": "libs/sdk-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "sdk-demo": "apps/sdk-demo",
    "search-widget": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "libs/search-widget",
      "sourceRoot": "libs/search-widget/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/search-widget/**/*.{ts,svelte,spec.ts}"]
          }
        },
        "check": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "svelte-check",
            "cwd": "libs/search-widget"
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "yarn vite build -c=libs/search-widget/vite.config.js && cp -r libs/search-widget/public/i18n dist/libs/search-widget"
            ]
          },
          "configurations": {
            "production": {}
          }
        },
        "build-original": {
          "executor": "@nxext/svelte:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/search-widget",
            "entryFile": "libs/search-widget/src/index.ts",
            "tsConfig": "libs/search-widget/tsconfig.lib.json",
            "svelteConfig": "libs/search-widget/svelte.config.cjs",
            "assets": [
              {
                "glob": "/*",
                "input": "./public/**",
                "output": "./"
              }
            ]
          },
          "configurations": {
            "production": {
              "dev": false
            }
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/search-widget"],
          "options": {
            "jestConfig": "libs/search-widget/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "search-widget-demo": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "apps/search-widget-demo",
      "sourceRoot": "apps/search-widget-demo/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nxext/vite:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/search-widget-demo",
            "assets": [
              {
                "glob": "/*",
                "input": "./public/**",
                "output": "./"
              }
            ],
            "tsConfig": "apps/search-widget-demo/tsconfig.app.json"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nxext/vite:dev",
          "options": {
            "outputPath": "dist/apps/search-widget-demo"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/search-widget-demo/**/*.{ts,svelte,spec.ts}"]
          }
        },
        "check": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "svelte-check",
            "cwd": "apps/search-widget-demo"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/search-widget-demo"],
          "options": {
            "jestConfig": "apps/search-widget-demo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sistema": "libs/sistema",
    "sistema-demo": "apps/sistema-demo"
  }
}
