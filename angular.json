      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush"
        }
      },
      "root": "apps/dashboard",
      "sourceRoot": "apps/dashboard/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/dashboard",
            "index": "apps/dashboard/src/index.html",
            "main": "apps/dashboard/src/main.ts",
            "polyfills": "apps/dashboard/src/polyfills.ts",
            "tsConfig": "apps/dashboard/tsconfig.app.json",
            "allowedCommonJsDependencies": [],
            "assets": [
              "apps/dashboard/src/favicon.ico",
              "apps/dashboard/src/assets",
              "apps/dashboard/src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "libs/pastanaga/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "libs/common/src/assets/i18n",
                "output": "assets/i18n"
              },
              {
                "glob": "**/*",
                "input": "apps/dashboard/src/environments_config/local",
                "output": "assets/deployment"
              },
              {
                "glob": "**/*",
                "input": "libs/sistema/assets",
                "output": "assets"
              }
            ],
            "styles": ["apps/dashboard/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/dashboard/src"]
            },
            "scripts": [],
            "ngswConfigPath": "apps/dashboard/src/ngsw-config.json",
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "30mb"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "50kb",
                "maximumError": "100kb"
              }
            ]
          },
          "configurations": {
            "production": {
              "outputHashing": "all",
              "serviceWorker": true,
              "fileReplacements": [
                {
                  "replace": "apps/dashboard/src/environments/environment.ts",
                  "with": "apps/dashboard/src/environments/environment.prod.ts"
                }
              ],
              "assets": [
                "apps/dashboard/src/favicon.ico",
                "apps/dashboard/src/assets",
                "apps/dashboard/src/manifest.webmanifest",
                {
                  "glob": "**/*",
                  "input": "libs/pastanaga/src/assets",
                  "output": "assets"
                },
                {
                  "glob": "**/*",
                  "input": "libs/common/src/assets/i18n",
                  "output": "assets/i18n"
                },
                {
                  "glob": "**/*",
                  "input": "apps/dashboard/src/environments_config/production",
                  "output": "assets/deployment"
                },
                {
                  "glob": "**/*",
                  "input": "libs/sistema/assets",
                  "output": "assets"
                }
              ]
            },
            "local-stage": {
              "aot": false,
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "assets": [
                "apps/dashboard/src/favicon.ico",
                "apps/dashboard/src/assets",
                "apps/dashboard/src/manifest.webmanifest",
                {
                  "glob": "**/*",
                  "input": "libs/pastanaga/src/assets",
                  "output": "assets"
                },
                {
                  "glob": "**/*",
                  "input": "libs/common/src/assets/i18n",
                  "output": "assets/i18n"
                },
                {
                  "glob": "**/*",
                  "input": "apps/dashboard/src/environments_config/local-stage",
                  "output": "assets/deployment"
                },
                {
                  "glob": "**/*",
                  "input": "libs/sistema/assets",
                  "output": "assets"
                }
              ]
            },
            "defaultConfiguration": "production"
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dashboard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dashboard:build:production"
            },
            "local-stage": {
              "browserTarget": "dashboard:build:local-stage"
            }
          },
          "defaultConfiguration": "local-stage"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dashboard:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dashboard"],
          "options": {
            "jestConfig": "apps/dashboard/jest.config.js",
            "passWithNoTests": true
