{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/sdk-demo/src",
  "prefix": "sdk-demo",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/sdk-demo",
        "index": "apps/sdk-demo/src/index.html",
        "main": "apps/sdk-demo/src/main.ts",
        "polyfills": "apps/sdk-demo/src/polyfills.ts",
        "tsConfig": "apps/sdk-demo/tsconfig.app.json",
        "aot": true,
        "assets": ["apps/sdk-demo/src/favicon.ico", "apps/sdk-demo/src/assets"],
        "styles": ["apps/sdk-demo/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/sdk-demo/src/environments/environment.ts",
              "with": "apps/sdk-demo/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "sdk-demo:build"
      },
      "configurations": {
        "development": {
          "browserTarget": "sdk-demo:build:development"
        },
        "production": {
          "browserTarget": "sdk-demo:build:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "sdk-demo:build"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/sdk-demo"],
      "options": {
        "jestConfig": "apps/sdk-demo/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
