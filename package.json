    "i18n-editor:extract": "ngx-translate-extract --input ./apps/editor/src ./libs/**/src --output ./apps/editor/src/assets/i18n/{en,ca,es}.json --clean --format json",
    "public-ssr:serve": "node dist/public-public/server/main.js",
    "workspace-generator": "nx workspace-generator",
    "sdk-build": "run-script-os",
    "sdk-build:win32": "echo 'NO WIN32 BUILD'",
    "sdk-build:default": "rimraf ./dist/sdk-core && cd ./libs/sdk-core && rollup -c && cd -",
    "postsdk-build:default": "yarn sdk-build-node",
    "sdk-build-node": "rimraf ./dist/sdk-core-node && cd ./libs/sdk-core/node && rollup -c && cd -",
    "widget:build": "ng build widget --prod --single-bundle --output-hashing none",
    "postinstall": "ngcc --properties es2020 browser module main"
