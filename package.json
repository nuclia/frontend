    "workspace-generator": "nx workspace-generator",
    "sdk:build": "rimraf ./dist/sdk-core && cd ./libs/sdk-core && rollup -c && cp package.json README.md ../../dist/sdk-core && cd - && cp ./LICENSE.md dist/sdk-core",
    "widget:build": "ng build widget --prod --single-bundle --output-hashing none"
