  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "nx run-many --all --target=test",
    "lint": "nx workspace-lint && ng lint",
    "update": "nx migrate latest",
    "help": "nx help",
    "missdev": "npx missdev --output=../libs --https --default-to-master",
    "sdk-build": "run-script-os",
    "sdk-build:win32": "echo 'NO WIN32 BUILD'",
    "sdk-build:default": "rimraf ./dist/sdk-core && cd ./libs/sdk-core && rollup -c && cd -",
    "postsdk-build:default": "yarn sdk-build-node",
    "sdk-build-node": "rimraf ./dist/sdk-core-node && cd ./libs/sdk-core/node && rollup -c && cd -",
    "postinstall": "ngcc --properties es2020 browser module main",
    "build:search-main-widget": "vite build -c=libs/search-widget/vite.config.js && cp -r libs/search-widget/public/i18n dist/libs/search-widget",
    "build:search-video-widget": "vite build -c=libs/search-widget/vite_video.config.js && mv dist/libs/search-video-widget/style.css dist/libs/search-video-widget/nuclia-video-widget.css",
    "build:search-widgets": "yarn run build:search-main-widget && yarn run build:search-video-widget && cp dist/libs/search-video-widget/* dist/libs/search-widget",
    "postbuild:search-widgets": "rm -rf dist/libs/search-video-widget"
  },
